# ~/.XCompose
include "/usr/share/X11/locale/en_US.UTF-8/Compose"

<Multi_key> <a> <l> <p> <h> <a>			: "α" # Compose alpha
<Multi_key> <b> <e> <t> <a>			: "β" # Compose beta
<Multi_key> <g> <a> <m> <m> <a>			: "γ" # Compose gamma
<Multi_key> <d> <e> <l> <t> <a>			: "δ" # Compose delta
<Multi_key> <D> <e> <l> <t> <a>			: "Δ" # Compose Delta
<Multi_key> <e> <p> <s>				: "ε" # Compose eps
<Multi_key> <e> <p> <s> <i> <l> <o> <n>		: "ε" # Compose epsilon
<Multi_key> <z> <e> <t> <a>			: "ζ" # Compose zeta
<Multi_key> <e> <t> <a>				: "η" # Compose eta
<Multi_key> <t> <h> <e> <t> <a>			: "θ" # Compose theta
<Multi_key> <i> <o> <t> <a>			: "ι" # Compose iota
<Multi_key> <k> <a> <p> <p> <a>			: "κ" # Compose kappa
<Multi_key> <l> <a> <m> <d> <a>			: "λ" # Compose lamda
<Multi_key> <l> <a> <m> <b> <d> <a>		: "λ" # Compose lambda
<Multi_key> <m> <u>				: "μ" # Compose mu
<Multi_key> <n> <u>				: "ν" # Compose nu
<Multi_key> <x> <i>				: "ξ" # Compose xi
<Multi_key> <o> <m> <i> <c> <r> <o> <n>		: "ο" # Compose omicron
<Multi_key> <p> <i>				: "π" # Compose pi
<Multi_key> <r> <h> <o>				: "ρ" # Compose rho
<Multi_key> <s> <i> <g> <m> <a>			: "σ" # Compose sigma
<Multi_key> <t> <a> <u>				: "τ" # Compose tau
<Multi_key> <u> <p> <s> <i> <l> <o> <n>		: "υ" # Compose upsilon
<Multi_key> <p> <h> <i>				: "φ" # Compose phi
<Multi_key> <c> <h> <i>				: "χ" # Compose chi
<Multi_key> <p> <s> <i>				: "ψ" # Compose psi
<Multi_key> <o> <m> <e> <g> <a>			: "ω" # Compose omega

<Multi_key> <o> <1>                             : "①" # Compose o1
<Multi_key> <o> <2>                             : "②" # Compose o2
<Multi_key> <o> <3>                             : "③" # Compose o3
<Multi_key> <o> <4>                             : "④" # Compose o4
<Multi_key> <o> <5>                             : "⑤" # Compose o5
<Multi_key> <o> <6>                             : "⑥" # Compose o6
<Multi_key> <o> <7>                             : "⑦" # Compose o7
<Multi_key> <o> <8>                             : "⑧" # Compose o8
<Multi_key> <o> <9>                             : "⑨" # Compose o9
<Multi_key> <o> <0> <1>                         : "⑩" # Compose o01

<Multi_key> <v> <e> <c>                         : "⃗" # Compose vec

<Multi_key> <b> <b> <R>                         : "ℝ" # Compose bbR
<Multi_key> <c> <a> <l> <F>                     : "ℱ" # Compose calF

<Multi_key> <f> <o> <r> <a> <l> <l>		: "∀" # Compose forall
<Multi_key> <e> <x> <i> <s> <t> <s>		: "∃" # Compose exists
<Multi_key> <n> <e> <x> <i> <s> <t> <s>		: "∄" # Compose nexists
<Multi_key> <i> <n> <s> <e> <t>			: "∈" # Compose inset
<Multi_key> <n> <o> <t> <i> <n>			: "∉" # Compose notin
<Multi_key> <s> <u> <m>				: "∑" # Compose sum
<Multi_key> <i> <n> <t>				: "∫" # Compose int
<Multi_key> <o> <i> <n> <t>			: "∮" # Compose oint
<Multi_key> <n> <e>                             : "≠" # Compose ne
<Multi_key> <l> <e>				: "≤" # Compose le
<Multi_key> <g> <e>				: "≥" # Compose ge
<Multi_key> <i> <n> <f>				: "∞" # Compose inf
<Multi_key> <p> <d>				: "∂" # Compose pd
<Multi_key> <t> <o>				: "→" # Compose to
<Multi_key> <f> <period>                        : "∘" # Compose f. (composite function)
<Multi_key> <equal> <greater>			: "⇒" # Compose =>
#<Multi_key> <less> <equal> <greater>		: "⇔" # Compose <=>
